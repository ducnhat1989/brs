<tr>
	<td><%= user.id %></td>
	<td><%= link_to user.email, admin_user_path(user) %>
	<td><%= link_to user.username, admin_user_path(user) %>
	</td>
	<td><%= user.created_at%></td>
	<td><%= link_to "Reset password", edit_admin_user_path(user),
	                              class: "btn btn-small btn-primary" %></td>
	<td><%= link_to "Delete", admin_user_path(user), method: :delete,
                                  data: { confirm: "You sure?" },
                                  class: "btn btn-small btn-primary"%>
</tr>